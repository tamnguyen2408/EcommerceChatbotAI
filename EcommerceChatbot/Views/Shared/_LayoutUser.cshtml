                                                            cartBadge.textContent = totalItems; // Hiển thị số lượng
                                                            cartBadge.style.display = 'inline'; // Hiện badge
                                                        } else {
                                                            cartBadge.style.display = 'none'; // Ẩn badge
                                                        }
                                                    })
                                                    .catch(error => console.error('Error fetching cart item count:', error));
                                            }

                                            // Gọi hàm cập nhật ngay khi trang load
                                            updateCartItemCount();
                                        });
