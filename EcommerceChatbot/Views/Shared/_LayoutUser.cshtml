        <df-messenger intent="WELCOME"
                      chat-title="Chat with Us"
                      agent-id="05a0fb92-45d2-44fc-896f-f2e558e309c0"
                      language-code="vi"></df-messenger>

        <style>
            df-messenger {
                --df-messenger-bot-message: #e9f1f7; /* Bot message background */
                --df-messenger-user-message: #d1e7dd; /* User message background */
                --df-messenger-font-color: #333; /* Text color */
                --df-messenger-button-color: #007bff; /* Button color */
                --df-messenger-button-title-color: white; /* Button text color */
                --df-messenger-title-color: #007bff; /* Chat title color */
                --df-messenger-input-color: #ffffff; /* Input background color */
                --df-messenger-input-font-color: #333; /* Input text color */
                --df-messenger-input-border-color: #cccccc; /* Input border color */
                --df-messenger-avatar-background: #007bff; /* Avatar background color */
                --df-messenger-avatar-border-radius: 50%; /* Avatar border radius */
            }

                /* Avatar image for the support agent */
                df-messenger .bot-message {
                    background-image: url('~/images/products/avatar_chatbot.png'); /* Path to your avatar image */
                    background-size: cover; /* Ensures the image covers the entire background */
                    border-radius: 20px; /* Rounded corners for the message bubble */
                    padding: 10px; /* Space inside the message bubble */
                    color: #333; /* Text color for bot messages */
                    font-weight: bold; /* Make text bold for better visibility */
                }

                /* Style for user's messages */
                df-messenger .user-message {
                    border-radius: 20px;
                    background-color: #d1e7dd; /* User message color */
                    padding: 10px; /* Padding for user messages */
                }
        </style>
        <style>
